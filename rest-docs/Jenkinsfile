pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/3636adesh/spring-experiments.git'
            }
        }
        stage('Build') {
            steps {
                script {
                    dir('rest-docs') {
                        sh 'mvn clean install -DskipTests'
                    }
                }
            }
        }
    }
}
